<!-- views/friend_list.ejs -->

<!DOCTYPE html>
<html lang="ko">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>친구 목록</title>
</head>

<body>
    <h1>친구 목록</h1>
    <ul>
        <% for (let i = 0; i < friends.length; i++) { %>
            <li><%= friends[i] %></li>
        <% } %>
    </ul>
</body>

</html>
